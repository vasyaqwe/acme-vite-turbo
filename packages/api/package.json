{
   "name": "@acme/api",
   "version": "0.1.0",
   "private": true,
   "type": "module",
   "exports": {
      ".": {
         "types": "./dist/index.d.ts",
         "default": "./src/index.ts"
      },
      "./env": {
         "types": "./dist/env.d.ts",
         "default": "./src/env.ts"
      },
      "./auth": {
         "types": "./dist/auth.d.ts",
         "default": "./src/auth.ts"
      }
   },
   "license": "MIT",
   "scripts": {
      "build": "tsc",
