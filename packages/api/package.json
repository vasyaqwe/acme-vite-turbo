   "dependencies": {
      "@acme/db": "workspace:*",
      "@acme/emails": "workspace:*",
      "@lucia-auth/adapter-drizzle": "^1.0.7",
      "@t3-oss/env-core": "^0.10.1",
      "@trpc/server": "11.0.0-rc.403",
      "@unkey/ratelimit": "^0.1.10",
      "arctic": "^1.9.1",
      "hono": "4.4.5",
      "lucia": "^3.2.0",
      "oslo": "^1.2.0",
      "superjson": "2.2.1",
      "zod": "^3.23.8"
   },
   "devDependencies": {
      "@acme/tsconfig": "workspace:*",
      "typescript": "^5.4.5"
   }
}
